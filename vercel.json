{
    "version": 2,
    "builds": [
      {
        "src": "front-end/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "front-end/build" },
        "command": "npm run vercel-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "front-end/$1"
      }
    ]
  }
  